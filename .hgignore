# Use shell expansion syntax
syntax: glob

# Object files or editor backup files.
*.o
*~
*.pyc
*.so

# executable / python package directories
Multistrand
multistrand/*


#########################################
#                                       #
# Testing data files and parameter sets #
#                                       #
#########################################

syntax: regexp

# .out files are from speed tests, and .dat are pickled data files from various tests.
# .tgz is used to copy them across sometimes.
^testing.*\.dat$
^testing.*\.out$
^testing.*\.tgz$

# Log files from Multistrand (old) or Kinfold
^testing.*\.log$

# These text files are usualy plaintext sequences, etc. 
^testing/speed.*\.txt$

# Test sets from Nupack guys
^testing.*testSetSS.txt$
^testing.*testSetMS.txt$

# Vienna DNA parameter set raw file [cannot be included]
dna\.par$

# Output(s) of the energy tester
^testing/energy/res.txt$
^testing/energy/test.*\.results\.txt$

# Side effect of some energy computation commands
# [TODO: track it down and see if we can turn it off, it's most likely
#   via the subprocess call to ViennaRNA's RNAfold command.]
rna\.ps$

# Profiler-generated pdf's and other pieces.
^testing/profiling/.*\.pdf$
^testing/profiling/.*\.prof$
^testing/profiling/gmon.out$

#########################################
#                                       #
# Other build / autogenerated files     #
#                                       #
#########################################

syntax: glob

# Documentation builds
doc/build
